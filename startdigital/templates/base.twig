<!DOCTYPE html>
<!-- [if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif] -->
<!-- [if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif] -->
<!-- [if gt IE 9]><! -->
	<html
	class="no-js no-svg" {{ site.language_attributes }}> <!-- <![endif] -->
	{% block html_head_container %}
		{% include 'html-header.twig' %}
	{% endblock %}
	<body
		class="post-type-{{ post.post_type}} page-{{ post.title|replace({' ': '-'})|lower }} {{ body_class }} bg-white min-h-screen" data-template="base.twig">
		<!-- Google Tag Manager (noscript) -->
		<noscript>
			<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WMB7F42B" height="0" width="0" style="display:none;visibility:hidden"></iframe>
		</noscript>
		<!-- End Google Tag Manager (noscript) -->

		<a class="skip-link screen-reader-text" href="#content">{{ _e('Skip to content') }}</a>

		{# Turned off at request of Flora - leaving here in case it makes a comeback #}
		{# {% include "partial/presale-banner.twig" %} #}

		{% include "partial/flashsale-banner.twig" %}

		{% block header %}
			{% include 'partial/header.twig' %}
		{% endblock %}

		<main id="content" role="main">
			{% block hero %}
				{% include "partial/internal-banner.twig" %}
			{% endblock %}

			{% block content %}
				<div class="container py-16 md:py-32">
					<h3 class="opacity-20">Stay tuned for further content...</h3>
				</div>
			{% endblock %}
		</main>
		{% block footer %}
			{% include 'footer.twig' %}
			{{ function('wp_footer') }}
		{% endblock %}

		{% if fn('is_page', 'daily-exercise') %}
			{% include "partial/hiitfit/daily-exercise/queue-video-modal.twig" %}
		{% endif %}

		{# Extend this to add custom scripts to just one page/block #}
		{% block scripts %}{% endblock %}
	</body>
</html>
